{
  "name": "ship-components-collaspable",
  "version": "0.1.0",
  "description": "Show and Hide",
  "main": "src/Collaspable.jsx",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --max-warnings 0 src/*.jsx",
    "lint:fix": "./node_modules/.bin/eslint --fix src/*.jsx",
    "test": "npm run lint && ./node_modules/.bin/jest src/ --coverage --coveragePathIgnorePatterns .\\*css",
    "test:watch": "./node_modules/.bin/jest src/ --watch --coverage --coveragePathIgnorePatterns .\\*css",
    "coverage": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ship-components-collaspable.git"
  },
  "keywords": [
    "react",
    "babel",
    "webpack",
    "ship-components"
  ],
  "author": "Isaac Suttell <isaac.suttell@sony.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/ship-components-collaspable/issues"
  },
  "homepage": "https://github.com/ship-components/ship-components-collaspable#readme",
  "dependencies": {
    "classnames": ">=2",
    "prop-types": ">=15",
    "react": ">=15",
    "ship-components-icon": ">=0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": ">=4",
    "eslint-config-ship-components": "^1.4.2",
    "eslint-plugin-react": "^7.2.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "transform": {".*": "<rootDir>/node_modules/jest-css-modules"}
  }
}
